			"properties": {
				"description": "LS para conexão com lista do Sharepoint para carga das informações do SB3",
				"annotations": [],
				"type": "SharePointOnlineList",
				"typeProperties": {
					"siteUrl": "https://votorantimindustrial.sharepoint.com/sites/novastecnologiasvsa/legadosb3/",
					"tenantId": "49158df8-04df-4db2-a1ae-ee3949ca3984",
					"servicePrincipalId": "0be6c3d7-1156-4385-9524-9d37605c6ceb",
					"servicePrincipalKey": {
						"type": "SecureString",
						"value": "**********"
					}
				}
			},
			"dependsOn": []
		}
	]
}